{{#each arrangedContent}}
<div class="row-fluid">
  <div class="span8 offset2">
    <h3><a {{bindAttr href="this.url"}} class="title-link">{{this.title}}</a></h3>
  </div>
</div>
<div class="row-fluid">
  <div class="span3 offset2">
    <img {{bindAttr src="this.thumbnail_url"}}, class="thumbnail"></img>
  </div>
  <div class="span1">&nbsp;</div>
  <div class="span4">
    {{this.summary}}...
    <br/>
    <br/>
    <p class="shared-by">Shared by <i>@{{this.user_screen_name}}</i>, {{this.js_created_at}}</p>
  </div>
</div>
{{/each}}
